<%- include("header") -%>
    <div class="box" id="heading">
        <h1>
            <%= Today%> List :
        </h1>
    </div>
    <div class="box">
        <% for(i=0; i<newListItems.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newListItems[i] %>
                </p>
            </div>
            <% } %>
        <form action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit">+</button>
        </form>
    </div>
    <%- include("footer") -%>